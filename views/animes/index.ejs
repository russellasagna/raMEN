<%- include('../partials/header') %>

<main>
  <img src="<%= image %>">
  <div id="results" class="list">
    <% animes.forEach(function(a) { %>
      <div>
        <p><%= a.title %></p>
        <a href="/animes/<%= a._id %>"><img src="<%= a.large %>" alt="Details"><br></a><br><br>
      </div>
    <% }); %>
  </div>
</main>

<%- include('../partials/footer') %>